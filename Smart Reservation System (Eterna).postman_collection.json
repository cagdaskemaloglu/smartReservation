						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status kodu 200 olmalı ve token dönmeli\", function () {",
									"    pm.response.to.have.status(200);",
									"    pm.expect(pm.response.json().token).to.be.a('string');",
									"});",
									"",
									"var jsonData = pm.response.json();",
									"",
									"// Token'ı ortam değişkenine kaydetme",
									"if (jsonData.token) {",
									"    pm.environment.set(\"adminToken\", jsonData.token);",
									"    console.log(\"Başarılı! Admin JWT kaydedildi: \" + jsonData.token.substring(0, 15) + \"...\");",
									"}"
